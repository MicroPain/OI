#include<bits/stdc++.h>
#define MAX 10000
using namespace std;
int MY_jia(int a[],int b[]);
int MY_cheng(int a[],int b[],c[]);
int main()
{
	int n;
	scanf("%d",&n);
	int a[MAX],b[MAX],c[3];
	memset(a,0,sizeof(a));
	memset(b,0,sizeof(b));
	for(int i=1;i<=50;i++)
	{
		if(i<10)
			c[1]=i;
		else
		{
			c[1]=i%10;
			c[2]=i/10;	
		}
		for(int j=1;j<=i;j++)
		{
			MY_cheng()
		}
	}
	return 0;
} 



